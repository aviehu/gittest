{
  "name": "client-apps",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "build:login": "webpack --config src/login-client/webpack.config.js",
    "build:main": "webpack --config src/main-client/webpack.config.js",
    "build:server": "babel src --out-dir dist/app",
    "start:login:dev": "ENTRY=dev webpack-dev-server --config src/login-client/webpack.config.js",
    "start:login": "webpack -w --config src/login-client/webpack.config.js",
    "start:main:dev": "ENTRY=dev webpack-dev-server --config src/main-client/webpack.config.js",
    "start:main": "webpack -w --config src/main-client/webpack.config.js",
    "start:server": "nodemon",
    "start": "yarn run build:login && yarn run build:main && yarn run build:server && node dist/app/app-server/index.js",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-react": "7.0.0",
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "cheerio": "^1.0.0-rc.3",
    "env-var": "^4.0.0",
    "eventemitter3": "^3.1.2",
    "fastify": "^2.2.0",
    "fastify-cookie": "^3.0.2",
    "fastify-cors": "^2.1.3",
    "fastify-plugin": "^1.5.0",
    "fastify-static": "^2.4.0",
    "jss": "^9.8.7",
    "lodash": "^4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-jss": "^8.6.1",
    "uuid": "^3.3.2",
    "vm2": "^3.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "7.0.0",
    "@storybook/react": "^5.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-unicorn": "^8.0.2",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "lorem-ipsum": "^2.0.2",
    "nodemon": "^1.18.11",
    "prettier": "^1.17.0",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.3.1"
  }
}
